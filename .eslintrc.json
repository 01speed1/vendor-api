{
  "parserOptions": { "ecmaVersion": 2017 },
  "env": {"es6": true },
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier "],
  "rules": {
    "prettier/pretiter": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-process-exit": "off"
  }
}
